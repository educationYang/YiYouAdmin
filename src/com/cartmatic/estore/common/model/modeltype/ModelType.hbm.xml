<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.cartmatic.estore.common.model.modeltype.ModelType"
        table="model_type"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="modelTypeId"
            column="modelTypeId"
            type="java.lang.Integer"
            length="10"
        >
            <generator class="native"/>
        </id>
        
          <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="0"
            not-null="false"
            unique="false"
        />
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="writer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="writer"
            length="32"
            not-null="false"
            unique="false"
        />
        <property
            name="remarks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remarks"
            length="220"
            not-null="false"
            unique="false"
        />
        <property
            name="barOne"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="barOne"
            length="220"
            not-null="false"
            unique="false"
        />
        <property
            name="barTwo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="barTwo"
            length="220"
            not-null="false"
            unique="false"
        />
        <property
            name="barThree"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="barThree"
            length="220"
            not-null="false"
            unique="false"
        />
	<!-- exported bi-directional one-to-many association to Product -->
		<set name="models" table="model" lazy="true" inverse="true" cascade="save-update" order-by="modelId desc" where="status=1">
			<key>
				<column name="modelTypeId" />
			</key>
			<one-to-many class="com.cartmatic.estore.common.model.model" />
		</set>
		
    </class>
</hibernate-mapping>
